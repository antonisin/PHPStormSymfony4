<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="TypeScript" />
      <blacklist language="CoffeeScript">
        <added pattern="i18nConfigure(opt)" />
      </blacklist>
      <blacklist language="XML" />
      <blacklist language="JavaScript" />
      <blacklist language="PHP">
        <added pattern="\end(array)" />
        <added pattern="\Symfony\Component\Console\Command\Command.addOption(name,shortcut,mode,description,default)" />
        <added pattern="\sprintf(format,args,_)" />
        <added pattern="\error_log(message,message_type,destination,extra_headers)" />
        <added pattern="\DuoCircle\WHMCS\Common\Hooks\ImprovelyAPI.sendConversion(identity,revenue,label,params)" />
        <added pattern="\array_key_exists(key,search)" />
        <added pattern="\PhishProtection\APIException(message, code)" />
        <added pattern="\filter_var(variable,filter,options)" />
        <added pattern="\PhishProtection\Service\NotificationService.send2Hook(hook,propagatorEmail,logId)" />
        <added pattern="\PhishProtection\Service\NotificationService.send2Email(propagatorEmail,receivers,logId)" />
        <added pattern="\PhishProtection\Service\NotificationService.send2Email(receivers,propagatorEmail,logId)" />
        <added pattern="\PhishProtection\Service\NotificationService.send2Email(receivers,propagatorEmail,logId,params)" />
        <removed pattern="" />
      </blacklist>
      <blacklist language="YAML" />
    </blacklists>
    <option id="php.show.names.for.all.args" value="true" />
  </component>
</application>